

module Main where


readIntString :: String -> Int
readIntString x = read x :: Int


orFalse :: Bool -> Bool
orFalse = ((||) False)

checkAliceAndBobScores :: String -> [String]
checkAliceAndBobScores x = [ show . length . filter orFalse $  (>) x,
                             show . length . filter orFalse $  (<) x ] 


solve :: String -> String
solve = unwords . checkAliceAndBobScores 


main :: IO()
main = interact solve
